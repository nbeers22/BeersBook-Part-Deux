<!DOCTYPE html>
<html>
<head>
  <title>BeersBook</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
	<% if user_signed_in? %>
		<%= render 'layouts/navbar' %>
	<% else %>
		<%= render 'layouts/welcome' %>
	<% end %>
			<%= yield %> 
		</div>
	</div>
	<footer></footer>
</div>

</body>
</html>
